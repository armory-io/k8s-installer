{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "nginx",
        "namespace": "${NAMESPACE}",
        "labels": {
            "app": "nginx"
        },
        "annotations": {
            "cloud.google.com/load-balancer-type": "${LB_TYPE}"
        }
    },
    "spec": {
        "ports": [
            {
                "name": "http",
                "port": 80,
                "targetPort": 80,
                "protocol": "TCP"
            },
            {
                "name": "https",
                "port": 443,
                "targetPort": 443,
                "protocol": "TCP"
            },
            {
                "name": "api",
                "port": 8085,
                "targetPort": 8085,
                "protocol": "TCP"
            }
        ],
        "selector": {
            "app": "nginx"
        },
        "type": "${SERVICE_TYPE}"
    }
}
