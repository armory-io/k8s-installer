{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "redis",
    "namespace": "armory",
    "labels": {
      "app": "redis"
    }
  },
  "spec": {
    "ports": [
      {
        "name": "redis",
        "port": 6379,
        "targetPort": 6379,
        "protocol": "TCP"
      }
    ],
    "selector": {
      "app": "redis",
      "role": "master"
    },
    "type": "LoadBalancer"
  }
}
