{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "deck",
    "namespace": "${NAMESPACE}",
    "annotations": {
      "cloud.google.com/load-balancer-type": "${LB_TYPE}"
    }
  },
  "spec": {
    "selector": {
      "app": "deck"
    },
    "ports": [
      {
        "protocol": "TCP",
        "port": 80,
        "targetPort": 9000
      }
    ],
    "type": "LoadBalancer"
  }
}
